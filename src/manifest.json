	"homepage_url": "http://bradly.me/",
	"update_url": "http://bradly.me/YTDUpdates.xml",
	"permissions": ["http://*.youtube.com/*"],
	"content_security_policy": "default-src 'self' ",
	"content_scripts": [
		{
			"matches": ["https://*.youtube.com/*"],
			"js": ["bradlys-ytd-injector.js"]
		}
	],
